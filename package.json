    "@eslint/js": "^9.18.0",
    "@tsconfig/node20": "^20",
    "@tsconfig/strictest": "^2.0.5",
    "@types/content-type": "^1.1.8",
    "@types/node": "^22.10.6",
    "@typescript-eslint/parser": "^8.20.0",
    "@wmakeev/filtrex": "^6.2.0",
    "@wmakeev/simplex-context": "^6.7.0",
    "auto-changelog": "^2.5.0",
    "envalid": "^8.0.0",
    "eslint": "^9.18.0",
